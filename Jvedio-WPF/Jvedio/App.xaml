<Application
    x:Class="Jvedio.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:d1p1="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:local="clr-namespace:Jvedio"
    d1p1:Ignorable="d"
    StartupUri="WindowStartUp.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <!--  Lang  -->
                <ResourceDictionary Source="pack://application:,,,/SuperControls.Style;Component/Lang/zh-CN.xaml" />
                <ResourceDictionary Source="pack://application:,,,/Jvedio;Component/Core/Lang/zh-CN.xaml" />

                <!--  SuperControls  -->
                <ResourceDictionary Source="pack://application:,,,/SuperControls.Style;Component/XAML/Skin/DefaultColor.xaml" />
                <!--<ResourceDictionary Source="pack://application:,,,/SuperControls.Style;Component/XAML/Skin/White.xaml" />-->
                <ResourceDictionary Source="pack://application:,,,/SuperControls.Style;Component/Themes/SuperControls.xaml" />

                <!--  Main  -->
                <ResourceDictionary Source="pack://application:,,,/Jvedio;Component/CustomStyle/SuperControls/Basic.xaml" />
                <ResourceDictionary Source="pack://application:,,,/Jvedio;Component/CustomStyle/SuperControls/Pagination.xaml" />
                <ResourceDictionary Source="pack://application:,,,/Jvedio;Component/CustomStyle/SuperControls/PathRadioButton.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>



</Application>